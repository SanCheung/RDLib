<?xml version="1.0" ?>
<!--roundcorner="1,1"-->
<Window size="900,600" caption="0,0,0,30" sizebox="4,4,6,6" roundcorner="1,1"  >
  <!--Include & Default-->
  <Font name="宋体" size="12" />
  <Font name="宋体" size="9" />
  <Font name="宋体" size="16" />
  
  <Include source="ctrl/scrollbar.xml" />
  <Include source="font.xml" />
  <!--<Include source="ctrl.xml" />-->
  <Default name="Combo" value="normalimage=&quot;file='ctrl/Combo_nor.bmp' corner='2,2,24,2'&quot; hotimage=&quot;file='ctrl/Combo_over.bmp' corner='2,2,24,2'&quot; pushedimage=&quot;file='ctrl/Combo_over.bmp' corner='2,2,24,2'&quot; disabledimage=&quot;file='ctrl/Combo_disabled.bmp' corner='2,2,24,2'&quot;" />


  <VBox bkcolor="#ffFFFFFF">
    <!--top-->
    <HBox height="30" >
      <Control />
      <Button name="bn2" width="30" height="30" text="Test" hottextcolor="#FFFF0000" font="0" />
      <Control />
      <Button name="bnMax" width="60" height="30" text="最大化" hottextcolor="#FFFF0000" font="0" />
      <Control width="6" />
      <Button name="bnClose" width="30" height="30" text="关闭" hottextcolor="#FFFF0000" font="0" />
      <Control width="6" />
    </HBox>

    <Control height="2" bkcolor="#FFfa6d00" />
    
    <HBox>
      <!--left-->
      <VBox width="240" childpadding="1" name="vlLeftBar" >
        <Option name="bnL1" text="汇报材料" height="40" font="0" group="g1" selected="true"
                normalimage="swh/level1-normal.png" selectedimage="swh/level1-hover.png" hotimage="swh/level1-hover.png" />

        <Option name="bnL2" text="案件信息" height="40" font="0" group="g1"
                normalimage="swh/level1-normal.png" selectedimage="swh/level1-hover.png" hotimage="swh/level1-hover.png" />

        <VBox name="vlL2Sub" visible="false" >
          <HBox height="40" >
            <Control width="1" />
            <Option name="bnL2-1" text="基本信息" height="40" font="1" group="g2"
                normalimage="swh/level2-normal.png" selectedimage="swh/level2-hover.png" hotimage="swh/level2-hover.png" />
          </HBox>

          <Control height="1" />
          <HBox height="40" >
            <Control width="1" />
            <Option name="bnL2-2" text="开庭信息" height="40" font="1" group="g2"
                normalimage="swh/level2-normal.png" selectedimage="swh/level2-hover.png" hotimage="swh/level2-hover.png" />
          </HBox>

          <Control height="1" />
          <HBox height="40" >
            <Control width="1" />
            <Option name="bnL2-3" text="电子卷宗" height="40" font="1" group="g2"
                normalimage="swh/level2-normal.png" selectedimage="swh/level2-hover.png" hotimage="swh/level2-hover.png" />
          </HBox>

          <Control height="1" />
          <HBox height="40" >
            <Control width="1" />
            <Option name="bnL2-4" text="文书信息" height="40" font="1" group="g2"
                normalimage="swh/level2-normal.png" selectedimage="swh/level2-hover.png" hotimage="swh/level2-hover.png" />
          </HBox>

          <Control />
        </VBox>

        <Option name="bnL3" text="法律法规" height="40" font="0" group="g1"
                normalimage="swh/level1-normal.png" selectedimage="swh/level1-hover.png" hotimage="swh/level1-hover.png" />
        <Option name="bnL4" text="庭审录像" height="40" font="0"  group="g1"
                normalimage="swh/level1-normal.png" selectedimage="swh/level1-hover.png" hotimage="swh/level1-hover.png" />

        <Control height="100" />
      </VBox>

      <TBox name="tabMain">
        <VBox inset="5,5,5,5" borderround111="10, 10" bordersize="5" bordercolor="#FFff0000" focusbordercolor="#FF0000ff">
          <Control />
          <Combo name="cbClient" width="200" height="20" itemtextpadding="10,0,20,0" itemfont="1" >
            <ListLabelElement text="北京" />
            <ListLabelElement text="上海" />
            <ListLabelElement text="广州" />
            <ListLabelElement text="深圳" />
            <ListLabelElement text="杭州" />
            <ListLabelElement text="西安" />
            <ListLabelElement text="成都" />
          </Combo>
          <Control />
          <HBox height="30" >
            <Control />
            <Edit width="280" name="edAccount" normalimage ="file='ctrl/edit.png' corner='4,4,4,4'" />
            <Control />
          </HBox>
          <Control />
          <Text name="tx" showhtml="true" hcenter="false" />
          <Control />
          <HBox height="300" >
            <Control width="50" />
            <ocx />
            <Control width="50" />
          </HBox>
          
          <Control />
        </VBox>
        <VBox>
          <IconPathView />
        </VBox>
        <VBox>
          <D2View />
        </VBox>
      </TBox>
      

      <Control width="1" bkcolor="#FFfa6d00" />
      
      <HBox name="vlHandle" pos="240,0,272,600" float="true" bkcolor111="#FF9999ff" >
        <Control width="1" bkcolor="#FFfa6d00" />
        <VBox >
          <Control />
          <Button height="104" name="bnLeftHandle" normalimage="swh/left-normal.png" hotimage="swh/left-hover.png" pushedimage="swh/left-hover.png" />
          <Control />
        </VBox>
      </HBox>
      
    </HBox>
    <Control height="1" bkcolor="#FFfa6d00" />
  </VBox>
</Window>